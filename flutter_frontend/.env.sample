# Copy this file to `.env` and fill the values before running locally.
# These values are used by Flutter app to initialize Firebase manually.
# For Android/iOS with google-services files you may also rely on platform config.

FIREBASE_API_KEY=your_api_key
FIREBASE_APP_ID=your_app_id
FIREBASE_MESSAGING_SENDER_ID=your_sender_id
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_STORAGE_BUCKET=your_bucket.appspot.com

# When true, the app connects to Firebase local emulators (auth:9099, firestore:8080).
# For Android emulators, host 10.0.2.2 points to local machine.
USE_FIREBASE_EMULATOR=false
